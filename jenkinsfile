pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                git "https://github.com/Abhi-2K5-hub/jenkins-docker-demo.git"
            }
        }
        stage('install') {
            steps {
                sh "npm install"
            }
        }
        stage('test') {
            steps {
                echo 'running tests....'
            }
        }
    }
}